{% extends "base.j2" %}

{% set page_title = 'On Demand Validation Result' %}


{% block header %}
    {% if next %}
        <a href="{{ next }}" class="btn btn-default" style="float: right">Back to start</a>
    {% else %}
        <a href="{{ url('on_demand_validate') }}?series_tag_id={{ serie_validation.series_tag_id }}" class="btn btn-default" style="float: right">Back to validation</a>
    {% endif %}
    {{ super() }}
{% endblock %}


{% block content %}
    <table class="table table-striped" style="width:auto">
        <tr><th>Agrees</th><td>{{ 'yes' if raw_annotation.agrees_with_id else 'no' }}</td></tr>
        <tr><th>Best kappa</th><td>{{ raw_annotation.best_kappa }}</td></tr>
    </table>
{% endblock %}
