{% extends "base.j2" %}

{% set page_title = title %}


{% block content %}
<form class="form" role="form" method="post">
    {% csrf_token %}
    {{ form|bootstrap }}
    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="Save" />
    </div>
</form>
<script type="text/javascript">
    $('form.form :input:visible:first').focus();
</script>
{% endblock %}
